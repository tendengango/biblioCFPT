<h2>Checked Out Books</h2>
<table>
  <thead>
    <tr>
      <th>Isbn</th>
      <th>Title</th>
      <th>Authors</th>
      <th>Language</th>
      <th>Published</th>
      <th>Edition</th>
      
      
      <th>Summary</th>
     
      
      <th colspan="6"></th>
    </tr>
    </thead>
    <tbody>
    <% @checkout.each do |book| %>

      <tr>
        <td><%= book.isbn %></td>
        <td><%= book.title %></td>
        <td><%= book.authors %></td>
        <td><%= book.language %></td>
        <td><%= book.published %></td>
        <td><%= book.edition %></td>
        
        
        <td><%= book.summary %></td>
       
        
        
        <td><%= link_to 'Return Book', controller: "books", action: "returnBook", id: book %></td>
      </tr>
    <% end %>
    </tbody>        
</table>